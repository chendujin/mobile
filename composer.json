{
    "name": "chendujin/mobile",
    "description": "PHP版本的中国手机号码归属地查询",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5|^6|^7|^8|^9"
    },
    "keywords": ["mobile", "phone"],
    "license": "MIT",
    "homepage": "https://github.com/chendujin/mobile",
    "autoload": {
        "psr-4": {
            "Chendujin\\Mobile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chendujin\\Mobile\\Tests\\" : "tests/"
        }
    },
    "authors": [
        {
            "name": "chendujin",
            "email": "thomas.chen.jobs@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": ">=5.6"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Chendujin\\Mobile\\ServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
